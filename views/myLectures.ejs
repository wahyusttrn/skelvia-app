<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Skelvia</title>
</head>
<body>
  <h1>My Lectures</h1>
  <table border="1">
    <thead>
      <tr>
        <th>No.</th>
        <th>Image</th>
        <th>Title</th>
        <th>Difficulty</th>
        <th>Votes</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% data.Lectures.forEach((e, i) => { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><img src="<%= e.imageUrl %>" alt="<%= e.title %>" height="100"></td>
          <td><%= e.title %></td>
          <td><%= e.difficulty %></td>
          <td><%= e.upVote %></td>
          <td>
            <a href="/myLectures/<%= e.id %>">See Challenges</a>
          </td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>